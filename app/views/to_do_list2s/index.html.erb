<%- model_class = ToDoList2 -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:task_name) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @to_do_list2s.each do |to_do_list2| %>
      <tr>
        <td><%= link_to to_do_list2.id, to_do_list2_path(to_do_list2) %></td>
        <td><%= to_do_list2.task_name %></td>
        <td><%= to_do_list2.description %></td>
        <td><%= to_do_list2.status %></td>
        <td><%=l to_do_list2.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      to_do_list2_path(to_do_list2), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_to_do_list2_path(to_do_list2), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      to_do_list2_path(to_do_list2),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_to_do_list2_path,
            :class => 'btn btn-primary' %>
